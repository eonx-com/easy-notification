{
    "name": "eonx-com/easy-notification",
    "description": "Client for dispatching notifications at EonX",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "aws/aws-sdk-php": "^3.134",
        "nette/utils": "^3.1",
        "symfony/http-client": "^4.2 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.4",
        "laravel/lumen-framework": "^5.5",
        "symfony/symfony": "^4.2 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyNotification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyNotification\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    }
}
